# take input
rolls_input = input("Enter roll numbers: ").split()
rolls = []
for x in rolls_input:
    rolls.append(int(x))

key = int(input("Enter roll number to search: "))

# simple linear search
def linear(a, k):
    for item in a:
        if item == k:
            return True
    return False

# easiest sentinel search
def sentinel(a, k):
    n = len(a)

    last = a[n - 1]     # save last element
    a[n - 1] = k        # put key at last

    i = 0
    while a[i] != k:
        i += 1

    a[n - 1] = last     # restore last

    if i < n - 1:
        return True
    if last == k:
        return True
    return False

# output
print("Linear Search:", "Found" if linear(rolls, key) else "Not Found")
print("Sentinel Search:", "Found" if sentinel(rolls, key) else "Not Found")
